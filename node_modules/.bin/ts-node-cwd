#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/ts-node@10.7.0_17a82b5ac88a5de7094eac76b4edda13/node_modules/ts-node/dist/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/ts-node@10.7.0_17a82b5ac88a5de7094eac76b4edda13/node_modules/ts-node/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/ts-node@10.7.0_17a82b5ac88a5de7094eac76b4edda13/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules:/Users/rfarrer/projects/node_modules:/Users/rfarrer/node_modules:/Users/node_modules:/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/ts-node/dist/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/ts-node/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/ts-node@10.7.0_17a82b5ac88a5de7094eac76b4edda13/node_modules/ts-node/dist/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/ts-node@10.7.0_17a82b5ac88a5de7094eac76b4edda13/node_modules/ts-node/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/ts-node@10.7.0_17a82b5ac88a5de7094eac76b4edda13/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules:/Users/rfarrer/projects/node_modules:/Users/rfarrer/node_modules:/Users/node_modules:/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/ts-node/dist/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/ts-node/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-node/dist/bin-cwd.js" "$@"
else
  exec node  "$basedir/../ts-node/dist/bin-cwd.js" "$@"
fi
