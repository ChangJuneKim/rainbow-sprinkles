#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/bin/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/typescript@4.6.3/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules:/Users/rfarrer/projects/node_modules:/Users/rfarrer/node_modules:/Users/node_modules:/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/ts-jest@27.1.4_1a7a295883fc72da5121c34ec472fa0c/node_modules/ts-jest/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/ts-jest@27.1.4_1a7a295883fc72da5121c34ec472fa0c/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/bin/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/typescript@4.6.3/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules:/Users/rfarrer/projects/node_modules:/Users/rfarrer/node_modules:/Users/node_modules:/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/ts-jest@27.1.4_1a7a295883fc72da5121c34ec472fa0c/node_modules/ts-jest/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/ts-jest@27.1.4_1a7a295883fc72da5121c34ec472fa0c/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@4.6.3/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../../../typescript@4.6.3/node_modules/typescript/bin/tsserver" "$@"
fi
